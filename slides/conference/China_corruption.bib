@article{Zhu2017,
  title = {{{MNCs}}, Rents, and Corruption: Evidence from China},
  shorttitle = {{{MNCs}}, Rents, and Corruption},
  author = {Zhu, Boliang},
  date = {2017},
  journaltitle = {American Journal of Political Science},
  volume = {61},
  number = {1},
  pages = {84--99},
  issn = {1540-5907},
  doi = {10.1111/ajps.12259},
  abstract = {How does multinational corporation (MNC) activity affect corruption in developing countries? The existing literature tends to suggest that economic integration helps reduce corruption, as it increases market competition and efficiency and promotes the diffusion of good governance. In this article, I argue that such a generalization oversimplifies the consequences of MNC activity in host countries. The entry and presence of MNCs may contribute to rent creation in developing countries, thereby leading to a high level of corruption. To test this argument, I conduct a case study on China and draw from original data of filed corruption cases to construct measures of corruption. I find that provinces with more MNC activity are strongly associated with more corruption. The results are robust and consistent when possible endogeneity, law enforcement, and alternative measures of corruption are considered. This finding has important implications for domestic governance in developing countries.},
  copyright = {\textcopyright 2016, Midwest Political Science Association},
  langid = {english},
  timestamp = {2020-02-12T18:02:26Z},
  file = {D:\Nutstore\01_Literature\Zhu2017_MNCs,_Rents,_and_Corruption_-_Evidence_from_China.pdf}
}

