@article{ZhaoJuanEtAl2023,
  ids = {ZhaoJuanEtAl2023a},
  title = {趋同性与多样性：数字政府政策扩散模式与影响机制------一个事件史分析},
  author = {{赵娟} and {杨泽森} and {张小劲}},
