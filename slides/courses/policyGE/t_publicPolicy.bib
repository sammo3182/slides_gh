  number = {01},
  pages = {18--25},
  issn = {1008-2204},
  abstract = {大数据时代的到来,集中体现为复杂多变的治理环境向传统的政府治理能力提出了挑战,但强大新颖的科技创新又为政府治理转型及其能力提升准备了必要的基础条件。"大数据驱动的政府治理能力"依托于治理理论和实践的发展,结合了互联网技术革命,是一种互联网和海量数据支撑的、涵盖了更广泛内涵的政府治理能力,概括了政府治理能力建设在大数据时代所呈现出的新发展、新动态和新前景,这集中反映在功能拓展、技术支撑和应用创新三方面。结合理论发展和实践应用,当前中国应大力强化基于大数据的开放政府、智慧政府、回应政府和濡化政府等模式创新,以全面提升政府决策能力、吸纳能力、整合能力、濡化能力和传统政府能力。},
  langid = {chinese},
  lccn = {11-3979/C},
  keywords = {,big data,big data driven,government capacity,government innovation,scientific technology innovation},
  timestamp = {2019-11-15T06:50:42Z}
}

@article{MengTianGuangZhaoJuan2018,
  title = {大数据驱动的智能化社会治理:理论建构与治理体系},
  shorttitle = {大数据驱动的智能化社会治理},
  author = {{孟天广} and {赵娟}},
